<?xml version="1.0" encoding="ISO-8859-1"?>
<cppStream
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="cppStream.xsd">
    <comsumers>
        <comsumer>
            <topic_list>
                <topic>fluent-newData</topic>
            </topic_list>
            <global_conf>
                <confEntry>
                    <entryKey>group.id</entryKey>
                    <entryValue>test_group_id</entryValue>
                </confEntry>
                <confEntry>
                    <entryKey>metadata.broker.list</entryKey>
                    <entryValue>kafka:9092</entryValue>
                </confEntry>
            </global_conf>
        </comsumer>
    </comsumers>
    <producers>
        <producer>
            <topic_list>
                <topic>rdkafkaPtest</topic>
            </topic_list>
            <global_conf>
                <confEntry>
                    <entryKey>metadata.broker.list</entryKey>
                    <entryValue>kafka:9092</entryValue>
                </confEntry>
            </global_conf>
        </producer>
    </producers>
</cppStream>
