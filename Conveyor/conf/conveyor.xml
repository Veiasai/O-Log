<?xml version="1.0" encoding="ISO-8859-1"?>
<conveyor
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="conveyor.xsd">
    <hostId>opdlvm0001</hostId>
    <quotas>
        <quota>1024*1024*2</quota>
        <quota>1024*1024*4</quota>
    </quotas>
    <groups>
        <group>
            <worker>
                <filePath>/olog/src/github.com/veiasai/olog/PythonScript/DataDir/rb1000.log</filePath>
                <brokers>kafka:9092</brokers>
                <topic>rb1000</topic>
                <backupType>disk</backupType>
                <filters>
                    <filter>
                        <!-- <filterType>empty</filterType> -->
                        <filterType>regex</filterType>
                        <filterRule>^[0-9]{13} \[Info  \] \[DATA_RECORDER\]*</filterRule>
                    </filter>
                </filters>
            </worker>
